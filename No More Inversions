void solve () {
    int n=0,m=0,a=0,b=0,c=0,x=0,y=0,z=0,w=0,k=0;
    cin>>n>>m;
    rep(i,1,m+m-n) cout<<i<<" ";
    repr(i,m,m+m-n-1) cout<<i<<" ";
    cout<<en;
}

