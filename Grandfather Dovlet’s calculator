void solve () {
    int n=0,m=0,a=0,b=0,x=0,y=0,z=0,w=0,k=0;
    cin>>a>>b;
    while(a<=b) {
        string s=to_string(a);
        rep(i,0,s.size()) {
            if(s[i]=='0') x+=6;
            else if(s[i]=='1') x+=2;
            else if(s[i]=='2') x+=5;
            else if(s[i]=='3') x+=5;
            else if(s[i]=='4') x+=4;
            else if(s[i]=='5') x+=5;
            else if(s[i]=='6') x+=6;
            else if(s[i]=='7') x+=3;
            else if(s[i]=='8') x+=7;
            else x+=6;
        }
        a++;
    }
    cout<<x;
}

inline void runn(){
	#ifndef ONLINE JUDGE
	freopen("input.txt","r",stdin);
	freopen("output.txt","w",stdout);
	#endif
}

signed main()
{
	night_rider
	//runn();
	int t=1;
	//cin>>t;
	rep(i,1,t+1) {
        // cout << "Case #" << i << ": ";
        solve();
    }
}
