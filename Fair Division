void solve () {
    int n=0,m=0,a=0,b=0,c=0,x=0,y=0,z=0,w=0,k=0;
    cin>>n;
    rep(i,0,n) {
        cin>>w;
        if(w%2==0) x++;
        else y++;
    }
    if(y%2) cout<<"NO"<<en;
    else if(x%2 && y==0) cout<<"NO"<<en;
    else cout<<"YES"<<en;
}

