void solve () {
    int n=0,m=0,a=0,b=0,c=0,x=0,y=0,z=0,w=0,k=0;
    cin>>n;
    cout<<2<<en;
    if(n==1) return ;
    x=2;
    rep(i,2,n+1) {
        k=(i*i*i+2*i*i+1);
        cout<<k<<en;
    }
}

